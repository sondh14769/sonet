{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": {
         "19": "logo.png"
      }
   }, 
	"content_scripts":[
		{
			"matches": ["<all_urls>"],
			"js":["js/jquery-1.10.1.js","js/contentscript.js"]
		},
		{
			"all_frames": true,
			"js":["js/jquery-1.10.1.js","js/contentscript.js"],
			"matches": [ "*://*/*" ]
		}
	],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "description": "Sonet app",
   "icons": {
      "128": "logo.png",
      "16": "logo.png",
      "48": "logo.png"
   },
   "manifest_version": 2,
   "name": "Sonet share app",
   "permissions": [ "tabs", "http://*/*", "https://*/*", "contextMenus" ],
   "version": "1.0",
   "web_accessible_resources": [ "images/facebook.jpeg", "images/google.png", "images/sonetLogo.jpeg", "images/twitter.png" ]
}
